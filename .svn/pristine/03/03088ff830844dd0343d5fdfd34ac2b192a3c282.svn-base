<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>购房提取 -- 人员信息录入</title>
</head>
<body>

	<form action="" method="POST">

		<table>
			<tr>
				<td>证件类型 ：</td>
				<td><select name="" id="">
						<option>身份证</option>
						<option></option>
				</select></td>
				<td>证件号码：</td>
				<td><input type="text" name="" id="" /></td>
				<td><input type="button" name="" id="" value="读卡" /></td>
			</tr>
			
			<tr>
				<td>公积金账号：</td>
				<td><input type="text" name="" id="" /></td>
				<td>姓名：</td>
				<td><input type="text" name="" id="" /></td>
			</tr>
			
			<tr>
				<td>业务类型：</td>
				<td><select name="" id="">
						<option>购房提取</option>
						<option></option>
				     </select>
				</td>
				<td>与产权人关系：</td>
				<td><select name="" id="">
						<option>本人</option>
						<option></option>
				     </select></td>
			</tr>

            <tr>
				<td>产证日期：</td>
				<td><input type="text" name="" id="" /></td>
			</tr>
			
			<tr>
				<td>政治面貌：</td>
				<td><select name="" id="">
						<option>群众</option>
						<option></option>
				     </select>
				</td>
				<td>职务：</td>
				<td><input type="text" name="" id="" /></td>
			</tr>
			
			<tr>
				<td>婚姻情况（选填）：</td>
			</tr>
			
			<tr>
				<td>配偶证件类型 ：</td>
				<td><select name="" id="">
						<option>身份证</option>
						<option></option>
				</select></td>
				<td>配偶证件号码：</td>
				<td><input type="text" name="" id="" /></td>
				<td><input type="button" name="" id="" value="读卡" /></td>
			</tr>
		</table>
	</form>



</body>
</html>